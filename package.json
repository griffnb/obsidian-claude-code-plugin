{
  "name": "obsidian-claude-code-plugin",
  "version": "1.0.0",
  "description": "Claude Code integration for Obsidian",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "build:css": "bunx postcss src/styles.css -o build/styles.css",
    "dev": "concurrently \"node esbuild.config.mjs\" \"bunx postcss src/styles.css -o build/styles.css --watch\"",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && bun run build:css",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.22",
    "builtin-modules": "3.3.0",
    "concurrently": "^9.2.1",
    "esbuild": "0.17.3",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^3.4.18",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "typescript-eslint": "^8.47.0"
  },
  "dependencies": {
    "obsidian": "latest",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7"
  }
}
